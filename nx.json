{
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "canTrackAnalytics": false,
        "showUsageWarnings": true
      }
    }
  },
  "targetDefaults": {
    "build": {
      "inputs": [
        "default",
        {
          "input": "self",
          "dependencies": true
        },
        {
          "externalDependencies": []
        }
      ],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "test": {
      "inputs": [
        "default",
        {
          "input": "self",
          "dependencies": true
        },
        {
          "externalDependencies": []
        }
      ],
      "cache": true
    },
    "lint": {
      "inputs": [
        "default",
        {
          "externalDependencies": ["eslint", "@eslint/js", "typescript-eslint"]
        },
        "{workspaceRoot}/eslint.config.js"
      ],
      "cache": true
    },
    "typecheck": {
      "inputs": [
        "default",
        {
          "input": "self",
          "dependencies": true
        },
        {
          "externalDependencies": []
        }
      ],
      "cache": true
    }
  },
  "defaultBase": "origin/alternative-main",
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": ["{projectRoot}/**/*"],
    "production": ["default"]
  },
  "useInferencePlugins": false
}
